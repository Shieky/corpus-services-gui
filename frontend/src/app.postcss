/*place global styles here */
html,
body {
	@apply h-full overflow-hidden;
	background: rgba(var(--color-surface-50));
	@font-face {
		/* Reference name */
		font-family: 'Inter';
		/* For multiple files use commas, ex: url(), url(), ... */
		src: url('../static/fonts/Inter-VariableFont_slnt.ttf');
	}
}

.modal {
	@apply border border-primary-500;
}
